<%= form_for :pick, url: picks_path do |f| %>

  <p>
    <%= f.label "Week" %><br>
    <%= f.select :week, [['10','10']] %>
  </p>  

  <p>
    <%= f.label "Rose" %><br>
    <%= f.select :rose, [['Shawn B.','Shawn B.'],['Nick','Nick']] %>
  </p>

  <p>
    <%= f.label "Tears" %><br>
    <%= f.select :tears, [['Shawn B.','Shawn B.'],['Nick','Nick']] %>
  </p>
 
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>