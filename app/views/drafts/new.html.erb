<%= form_for :draft, url: drafts_path do |f| %>

  <p>
    <%= f.label "Round" %><br>
    <%= f.select :round, [['1', '1'],['2','2'],['3','3'],['4','4'],['Winner Pick','5']] %>
  </p>  

  <p>
    <%= f.label "Bachelorette" %><br>
    <%= f.select :bachelor, [['Shawn B.','Shawn B.'],['Chris','Chris'],['Ben H.','Ben H.'],['J.J','J.J'],['Joe','Joe'],['Kupah','Kupah'],['Daniel','Daniel'],['Ryan B.','Ryan B.'],['Joshua','Joshua'],['Tony','Tony'],['Clint','Clint'],['Corey (NY)','Corey (NY)'],['Jonathan','Jonathan'],['Cory (NC)','Cory (NC)'], ['Ben Z.','Ben Z.'],['Tanner','Tanner'],['Ian','Ian'],['Justin','Justin'],['Jared','Jared']] %>
  </p>
 
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>