<%= form_for :signup, url: signups_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
 
  <p>
    <%= f.label "Bachelorette" %><br>
    <%= f.select :bachelorette, [['Kaitlyn','Kaitlyn'],['Britt','Britt']] %>
  </p>

  <p>
    <%= f.submit "Sign Up" %>
  </p>
<% end %>